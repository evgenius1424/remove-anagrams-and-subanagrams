{
  "metadata": {
    "problem": "Remove Anagrams and Sub-Anagrams",
    "version": "1.0.0",
    "total_cases": 25
  },
  "test_cases": [
    {
      "id": 1,
      "category": "basic",
      "input": ["a", "ab", "ba", "abc", "abcd"],
      "expected": ["abcd"],
      "explanation": "ab/ba are anagrams (both removed), a/abc are sub-anagrams of abcd"
    },
    {
      "id": 2,
      "category": "basic",
      "input": ["cat", "dog", "bird"],
      "expected": ["cat", "dog", "bird"],
      "explanation": "No anagrams or sub-anagrams exist"
    },
    {
      "id": 3,
      "category": "basic",
      "input": ["abc", "def", "cba", "fed", "xyz"],
      "expected": ["xyz"],
      "explanation": "abc/cba and def/fed are anagram pairs, only xyz remains"
    },
    {
      "id": 4,
